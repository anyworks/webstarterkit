{
  "name": "webstarterkit",
  "version": "1.0.0",
  "description": "More easily and quickly be able to create a WebUI",
  "scripts": {
    "script": "node_modules/.bin/tsc --watch",
    "html": "node_modules/.bin/pug ./src --watch --out ./dist/ --pretty",
    "css": "node_modules/.bin/node-sass src/scss -w -o ./dist/css -r",
    "dev": "concurrently \"npm run script\" \"npm run html\" \"npm run css\"",
    "elec": "node_modules/.bin/electron dist/main.js"
  },
  "author": "anyworks",
  "license": "ISC",
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/node": "^13.7.1",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "concurrently": "^5.0.2",
    "node-sass": "^4.13.0",
    "pug": "^2.0.4",
    "pug-cli": "^1.0.0-alpha6",
    "typescript": "^3.7.3"
  },
  "dependencies": {
    "electron": "^8.0.0",
    "electron-json-storage": "^4.1.8"
  }
}
